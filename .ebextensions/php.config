option_settings:
  - namespace: 'aws:elasticbeanstalk:container:php:phpini'
    option_name: document_root
    value: /web
container_commands:
  01-install-packages:
    command: "composer.phar install"
  99-copy-install:
    command: /bin/cp /var/app/current/config/install.php /var/app/current/web/wp/wp-admin/install.php
